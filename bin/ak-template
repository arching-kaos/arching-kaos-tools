##
fullprogrampath="$(realpath $0)"
PROGRAM=$(basename $0)
descriptionString="Quick description"

# At least these
source $AK_LIBDIR/_ak_lib_load
_ak_lib_load _ak_log
_ak_lib_load _ak_script

# Your stuff here...
example(){
    echo "Example"
}

# Flags to run
if [ ! -z $1 ]
then
    case $1 in
        -h | --help) _ak_usage; exit;;
        --example) example; exit;;
        * ) _ak_usage;;
    esac
else _ak_usage
fi
